<div id='chat-grid'>
	<div id='chat-menu'>
		<h2>Chat</h2>
		<h5>Users</h5>
		{{#each @model.users as |user|}}
			{{ user.name }}
		{{/each}}<br><br>
		<h5>Channels</h5>
		{{#each @model.channels as |channel|}}
			<LinkTo @route="chat.channel" @model={{channel.id}}>#{{ channel.name }}</LinkTo><br>
		{{/each}}<br>
		<div>
			<a href="#" {{on "click" this.test}}>test</a>
			{{#if this.session.isAuthenticated }}
				LOGGED IN
			{{else}}
				LOGGED OUT
			{{/if}}
		</div>
	</div>

	{{outlet}}
</div>